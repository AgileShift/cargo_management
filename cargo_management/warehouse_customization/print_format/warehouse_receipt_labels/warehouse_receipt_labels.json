 "html": "{%- for row in doc.packages_in_warehouse_receipt -%}\n\n<div class=\"header-html\">\n\t<div class=\"print-heading\">\n\t\t<div class=\"row section-break\">\n\t\t\t<div class=\"col-xs-6 text-left\">\n\t\t\t\tQUICKBOX NICARAGUA<br>\n\t\t\t\toperations@quickboxni.com<br>\n\t\t\t\twww.quickboxni.com<br>\n\t\t\t</div>\n\t\t\t<div class=\"col-xs-6\">\n\t\t\t\t8534 NW 70TH ST<br>\n\t\t\t\tMiami, Florida, 33166-2652<br>\n\t\t\t\tPhone: +1 (786) 303-2784\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</div>\n\n<div class=\"row section-break\">\n\t<div class=\"col-xs-12 column-break\"> \n\t\t<label>SHIPPER</label>\n\t\t<h4 class=\"padding-left\">{{ doc.shipper }}</h4>\n\t</div>\n</div>\n\n<div class=\"row section-break\">\n\t<div class=\"col-xs-12 column-break\">\n\t\t<label>CONSIGNEE</label>\n\t\t<h4 class=\"padding-left\">{{ row.consignee or doc.consignee }}</h4>\n\t</div>\n</div>\n\n<div class=\"row section-break\">\n\t<div class=\"col-xs-12 column-break\">\n\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-6\">\n\t\t\t\t<label>WAREHOUSE RECEIPT #</label>\n\t\t\t</div>\n\t\t\t<div class=\"col-xs-6 text-right\">\n\t\t\t\t<label style=\"display: inline;\">DATE:</label>\n\t\t\t\t<small>{{ row.receipt_date or doc.opening_date }}</small>\n\t\t\t</div>\n\t\t</div>\n\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-12 text-center\">\n\t\t\t\t<svg class=\"barcode\"\n        \t\t\tjsbarcode-width=\"2.5\"\n        \t\t\tjsbarcode-height=\"45\"\n        \t\t\tjsbarcode-fontoptions=\"bold\"\n        \t\t\tjsbarcode-textPosition=\"top\"\n        \t\t\tjsbarcode-textmargin=\"5\"\n        \t\t\tjsbarcode-fontsize=\"21\"\n        \t\t\tjsbarcode-margin=\"5\"\n        \t\t\tjsbarcode-marginbottom=\"5\"\n        \t\t\tjsbarcode-value=\"{{ doc.name }}\"></svg>\n\t\t\t</div>\n\t\t</div>\n\n\t</div>\n</div>\n\n<div class=\"row section-break\">\n\t<div class=\"col-xs-6\">\n\t\t<label style=\"margin-bottom: 0;\">SERVICE</label>\n\t\t<h1 class=\"text-center\">{{ doc.transportation_type|upper }}</h1>\n\t</div>\n\t<div class=\"col-xs-6 border-left\">\n\t\t<div class=\"row section-break\">\n\t\t\t<div class=\"col-xs-12\">\n\t\t\t\t<label>PCS</label>\n\t\t\t\t<h4>{{ loop.index }} of {{ loop.length }}</h4>\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"row\">\n\t\t\t<div class=\"col-xs-12\">\n\t\t\t\t<label>PACKAGE TYPE</label>\n\t\t\t\t<h4>{{ row.type }}</h4>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</div>\n\n<div class=\"row section-break\">\n\t<div class=\"col-xs-4\">\n\t\t<label>UNIT WEIGHT</label>\n\t\t<p>{{ row.weight }} lb</p>\n\t</div>\n\t<div class=\"col-xs-4 border-left\">\n\t\t<label style=\"font-size: 8pt;\">TOTAL WEIGHT</label>\n\t\t<p>{{ doc.total_weight }} lb</p>\n\t</div>\n\t<div class=\"col-xs-4 border-left\">\n\t\t<label>DIMENSIONES</label>\n\t\t<p>{{ row.get_formatted(\"length\", doc) }}x{{ row.get_formatted(\"width\", doc) }}x{{ row.get_formatted(\"height\", doc) }}</p>\n\t</div>\n</div>\n\n<div class=\"row section-break\">\n\t<div class=\"col-xs-12\">\n\t\t<label>DESCRIPTION</label>\n\t\t<p class=\"padding-left\">{{ row.get_formatted(\"description\", doc) }}</p>\n\t</div>\n</div>\n\n<div class=\"row\">\n\t<div class=\"col-xs-12 text-center\">\n\t\t{% if row.tracking_number %}\n\t\t\t<svg class=\"barcode\"\n    \t\t\tjsbarcode-width=\"1\"\n    \t\t\tjsbarcode-height=\"50\"\n    \t\t\tjsbarcode-textmargin=\"5\"\n    \t\t\tjsbarcode-fontsize=\"15\"\n    \t\t\tjsbarcode-margin=\"10\"\n    \t\t\tjsbarcode-marginbottom=\"1\"\n    \t\t\tjsbarcode-value=\"{{ row.tracking_number }}\"></svg>\n\t\t{% endif %}\n\t</div>\n</div>\n\n<div class=\"page-break\"></div>\n{%- endfor -%}\n\n<script src=\"https://cdn.jsdelivr.net/npm/jsbarcode@3.11.4/dist/JsBarcode.all.min.js\"></script>\n<script>JsBarcode(\".barcode\").init();</script>",
