 "html": "{%- for row in doc.warehouse_receipt_lines -%}\n\n<div class=\"header-html\">\n    <div class=\"print-heading\">\n        <div class=\"row section-break\">\n            <div class=\"col-xs-6 text-left\">\n                QUICKBOX NICARAGUA<br>\n                operations@quickboxni.com<br>\n                www.quickboxni.com<br>\n            </div>\n            <div class=\"col-xs-6\">\n                8534 NW 70TH ST<br>\n                Miami, Florida, 33166-2652<br>\n                Phone: +1 (786) 303-2784\n            </div>\n        </div>\n    </div>\n</div>\n\n<div class=\"row section-break\">\n    <div class=\"col-xs-12 column-break\"> \n        <label>SHIPPER</label>\n        <h4 class=\"padding-left\">{{ doc.shipper }}</h4>\n    </div>\n</div>\n\n<div class=\"row section-break\">\n    <div class=\"col-xs-12 column-break\">\n        <label>CONSIGNEE</label>\n        <h4 class=\"padding-left\">{{ row.consignee or doc.consignee }}</h4>\n    </div>\n</div>\n\n<div class=\"row section-break\">\n    <div class=\"col-xs-12 column-break\">\n\n        <div class=\"row\">\n            <div class=\"col-xs-6\">\n                <label>WAREHOUSE RECEIPT #</label>\n            </div>\n            <div class=\"col-xs-6 text-right\">\n                <label style=\"display: inline;\">DATE:</label>\n                <small>{{ row.receipt_date or doc.opening_date }}</small>\n            </div>\n        </div>\n\n        <div class=\"row\">\n            <div class=\"col-xs-12 text-center\">\n                <svg class=\"barcode\"\n                    jsbarcode-width=\"2.5\"\n                    jsbarcode-height=\"45\"\n                    jsbarcode-fontoptions=\"bold\"\n                    jsbarcode-textPosition=\"top\"\n                    jsbarcode-textmargin=\"5\"\n                    jsbarcode-fontsize=\"21\"\n                    jsbarcode-margin=\"5\"\n                    jsbarcode-marginbottom=\"5\"\n                    jsbarcode-value=\"{{ doc.name }}\"></svg>\n            </div>\n        </div>\n\n    </div>\n</div>\n\n<div class=\"row section-break\">\n    <div class=\"col-xs-6\">\n        <label style=\"margin-bottom: 0;\">SERVICE</label>\n        <h1 class=\"text-center\">{{ doc.transportation_type|upper }}</h1>\n    </div>\n    <div class=\"col-xs-6 border-left\">\n        <div class=\"row section-break\">\n            <div class=\"col-xs-12\">\n                <label>PCS</label>\n                <h4>{{ loop.index }} of {{ loop.length }}</h4>\n            </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-12\">\n                <label>PACKAGE TYPE</label>\n                <h4>{{ row.type }}</h4>\n            </div>\n        </div>\n    </div>\n</div>\n\n<div class=\"row section-break\">\n    <div class=\"col-xs-4\">\n        <label>UNIT WEIGHT</label>\n        <p>{{ row.warehouse_est_weight }} lb</p>\n    </div>\n    <div class=\"col-xs-4 border-left\">\n        <label style=\"font-size: 8pt;\">TOTAL WEIGHT</label>\n        <p>{{ doc.warehouse_est_gross_weight }} lb</p>\n    </div>\n    <div class=\"col-xs-4 border-left\">\n        <label>DIMENSIONES</label>\n        <p>{{ row.get_formatted(\"length\", doc) }}x{{ row.get_formatted(\"width\", doc) }}x{{ row.get_formatted(\"height\", doc) }}</p>\n    </div>\n</div>\n\n<div class=\"row section-break\">\n    <div class=\"col-xs-12\">\n        <label>DESCRIPTION</label>\n        <p class=\"padding-left\">{{ row.get_formatted(\"description\", doc) }}</p>\n    </div>\n</div>\n\n<div class=\"row\">\n    <div class=\"col-xs-12 text-center\">\n        {% if row.package %}\n            <svg class=\"barcode\"\n                jsbarcode-width=\"1\"\n                jsbarcode-height=\"50\"\n                jsbarcode-textmargin=\"5\"\n                jsbarcode-fontsize=\"15\"\n                jsbarcode-margin=\"10\"\n                jsbarcode-marginbottom=\"1\"\n                jsbarcode-value=\"{{ row.package }}\"></svg>\n        {% endif %}\n    </div>\n</div>\n\n<div class=\"page-break\"></div>\n{%- endfor -%}\n\n<script src=\"https://cdn.jsdelivr.net/npm/jsbarcode@3.11.4/dist/JsBarcode.all.min.js\"></script>\n<script>JsBarcode(\".barcode\").init();</script>",
